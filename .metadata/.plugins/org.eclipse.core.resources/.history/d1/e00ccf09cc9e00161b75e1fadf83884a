public class mainlp302{
	
	public static void head(int length, int height){
		for (int x = 1; x <= length; x++){
			System.out.print("-");
		}
		System.out.println(" ");
		
		
		for(int z = 1; z <= height; z++){
			System.out.print("|");
			for (int y = 1; y <= length; y++){
				System.out.print(" ");
			}
			System.out.println("|");
		}
}
		
		
	
	
	
	public static void main(String[] args){
		head(6,5);
		
		}
}